<html class=" web-homepage target-experience-full js">
   <!-- template for the pricing -->
   <!-- script for header -->
   <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
   <head>
      <title> Fearless </title>
      <script type="text/javascript">
         window.targetExperience = 'full';
      </script>
      <meta name="description" content="Fearless makes it painless for young women to experiment with bold, new styles and appreciate their bodies. We provides insider access to today's top brands at affordable prices.">
      <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
      <meta property="og:description" content="Fearless provides insider access to today's top brands for women, men, kids, and home as well as local experiences, amazing getaways, and gourmet finds - at up to 60% off">
      <meta property="og:type" content="website">
      <meta property="og:site_name" content="Fearless">
      <!-- insert favicon here -->
      <style type="text/css">
         /* This is for rogue tracking pixels that don't hide themselves */
         body > iframe,
         body > img {
         position: absolute;
         top: -30px;
         }
      </style>
      <!-- THIN HEADER
         ================================= -->
      <header id="header" class="header-section section section-dark navbar navbar-fixed-top">
         <div class="container-fluid">
            <div class="navbar-header navbar-left">
               <!-- RESPONSIVE MENU BUTTON -->
               <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation">
               <span class="sr-only">Toggle navigation</span>
               <span class="icon-bar"></span>
               <span class="icon-bar"></span>
               <span class="icon-bar"></span>
               </button>
               <!-- HEADER LOGO -->
               <a class="navbar-logo navbar-brand anchor-link" href="#hero">
               <%= image_tag("fearless-white-large.png") %>
               </a>
            </div>
            <nav id="navigation" class="navigation navbar-collapse collapse navbar-right">
               <!-- NAVIGATION LINKS -->
               <ul id="header-nav" class="nav navbar-nav">
                  <li><a href="#hero" class="hidden">Top</a></li>
                  <li><a href="#why"> WHY FEARLESS </a></li>
                  <li><a href="#how"> HOW IT WORKS </a></li>
                  <li><a href="#what"> THE PACKAGE </a></li>
                  <li><a href="#price">PRICE</a></li>
                  <li><a href="#love"> WE LOVE </a></li>
                  <!-- HEADER ACTION BUTTON -->
                  <li class="header-action-button"><a href="javascript:signUp();" class="button tint WOMEN">Sign Up</a></li>
               </ul>
            </nav>
         </div>
      </header>
      <!--- Google Analytics -->
      <script>
         (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
         (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
         m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
         })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
         
         ga('create', 'UA-60901045-1', 'auto');
         ga('send', 'pageview');
         
      </script>
      <!--<![endif]-->
   </head>
   <body class="homepage women" cz-shortcut-listen="true">
      <div class="nav-unified">
         <header class="fearless-navigation component-nav-unified login-reg-wall">
            <div class="nav-wrap">
               <div class="fearless-logo">
                  <a href="#" class="logo  login-reg-wall-override">fearless</a>
               </div>
               <div class="nav-unified-account-container">
                  <div class="nav-unified-account-inner">
                     <nav class="account-navigation">
                        <ul class="account-tabs">
                           <% if user_signed_in? %>
                           <!-- needs a method delete --> 
                           <!-- put has to do with updating -->
                           <%= link_to "Log Out", destroy_user_session_path, method: :delete, :class => 'account-nav-label  login-link'%>
                           <% else %>
                           <%= link_to "Sign Up", new_user_registration_path, :class => 'account-nav-label register-link' %>
                           <% end %> 
                           <!--
                              <li class="account-tab account-nav-sign-in">
                                  <a href="#" id="button_id" data-fearless-tracking="sign-in" class="account-nav-label  login-link">Sign In</a>
                              </li>
                              <li class="account-tab">
                                  <a href="javascript:signUp();" id="button_id" class="account-nav-label register-link" >Register</a>
                              </li> -->
                        </ul>
                     </nav>
                     <div class="nav-actions" onClick="javascript:signUp()">
                        <button class=" fearless-cart-open-link login-reg-wall-override" data-fearless-tracking="cart-label">
                        Cart
                        <span class="nav-cart-count cart-count nav-cart-item-count">
                        0
                        </span>
                        </button> 
                     </div>
                  </div>
               </div>
               <div class="nav-unified-store-container">
                  <div class="nav-unified-store-inner">
                     <form class="search search-form nav-unified-autosuggest search-form-deactivated search-menu-anchor" method="post"action="javascript:signUp();">
                        <div class="search-form-inner" id='button_id'>
                           <input class="search-input" type="search" tabindex="1" autocomplete="off" placeholder="Search" data-fearless-test="input-search" >
                           <button id="button_id" class="search-submit" type="submit" data-fearless-test="button-search">
                           <input class="search-clear" type="reset" value="Clear">
                           <button class="search-cancel" type="button">Cancel</button>
                        </div>
                     </form>
                  </div>
               </div>
            </div>
         </header>
      </div>

      <div class="main">
      <div id="user-logged-in" style="visibility: hidden; height: 0;" is-logged-in="false"></div>
      <section class="hero-container sales-store-affinity-hero" id="banner-img" onclick="javascript:signUp()">
         <section class="hero">
            <article class="sale sale-1101628453 sale-index-0" data-fearless-sale-id="1101628453">
               <div class="mwk-superhero">
                  <header class="name">
                     <h1 class="h1 WOMEN">
                        <a href="javascript: signUp()" class="sale-header-link" style="color:#000000">The Ultimate Dress Shop</a>
                     </h1>
                     <p class="copy" style="color:#000000">Effortlessly feminine staples for work, weekends, and beyond
                     </p>
                     <a class="button tint WOMEN"> Shop Fearless </a>
                  </header>
               </div>
            </article>
         </section>
      </section>
  </DIV>
    <div id="form_id">
    <aside class="dom-dialog-container dom-dialog-0 login-register-dialog dom-dialog-theme-nouveau dom-dialog-position-center dom-dialog-open" id="dom-dialog-0">
        <div class="dom-dialog-wrapper-1">
            <div class="dom-dialog-wrapper-2">
                <div class="dom-dialog">
                    <article class="dom-dialog-content">
                        <div class="login-register" style="display: block; ">
                            <div class="outer-pane">
                                <!-- Country:US-United States-65.112.10.59 -->
                                <!----><!-- Registration Form--><!---->
                                <br/>
                                <br/>
                                <div class="gilt-login-reg-logo"> <%= image_tag("fearless-black-resolution.png", :style => "max-height:80px") %> 
                                </div>
                                <div class="reg-form-content-container pane reg-form form">
                                    <h1 class="category-heading h1">Sign up for our exclusive membership.</h1>
                                    <div class="reg-form-content">
                                        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                                        <p style="error-message"> <%= devise_error_messages! %> </p>
                                        <!--<div ><%= f.label :email%><br />-->
                                        <%= f.email_field :email, autofocus: true, placeholder: "Email Address", :class => "primary email text required"%>
                                    </div>
                                    <div>
                                        <!--<%= f.label :password %><br />-->
                                        <%= f.password_field :password, autocomplete: "off", placeholder: "Password", :class => "primary email text required" %>
                                    </div>
                                    <div>
                                        <!--<%= f.label :password_confirmation %><br />-->
                                        <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Confirmation", :class => "primary email text required"%>
                                    </div>
                                    <%= f.submit "Join Now!", :class => "button join-now"%>
                                    <% end %>
                                    <div class="or-container">
                                        <h6 class="or">or</h6>
                                    </div>
                                    <div class="facebook-login">
                                        <button id="fb-button" class="start-sharing fb-button fb-login">
                                        Login with Facebook
                                        </button>
                                    </div>
                                    <p class="posting terms">
                                        We will never post on your behalf without
                                        first obtaining your explicit permission.
                                    </p>
                                    <p class="already-member">
                                        Already a member? 
                                        <span class="sign-in-link change-form"> <%= render "devise/shared/links" %> </span>

                                    </p>
                                    
                                    <BR/>
                                </div>
                            </div>
                        </div>
                    </article>
                </div>
            </div>
        </div>
    </aside>
    </div>
      <!-- end testing -->
   </body>
</html>